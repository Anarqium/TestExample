{
  "name": "Firefox Advanced Startpage",
  "short_name": "Startpage",
  "description": "A comprehensive Firefox startpage with widgets, weather, news, and productivity tools",
  "start_url": "./test.html",
  "display": "standalone",
  "background_color": "#1a1a1a",
  "theme_color": "#ff9500",
  "orientation": "portrait-primary",
  "categories": ["productivity", "utilities", "lifestyle"],
  "lang": "de-DE",
  "icons": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "192x192",
      "type": "image/svg+xml"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "512x512",
      "type": "image/svg+xml"
    }
  ],
  "shortcuts": [
    {
      "name": "Weather",
      "short_name": "Weather",
      "description": "Check current weather",
      "url": "./test.html?widget=weather",
      "icons": [{ "src": "/images/weather.png", "sizes": "96x96" }]
    },
    {
      "name": "Todo",
      "short_name": "Todo",
      "description": "Manage your tasks",
      "url": "./test.html?widget=todo",
      "icons": [{ "src": "/images/todo.png", "sizes": "96x96" }]
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "protocol_handlers": [
    {
      "protocol": "web+startpage",
      "url": "./test.html?url=%s"
    }
  ]
}